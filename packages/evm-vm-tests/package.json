{
  "name": "evm-vm-tests",
  "version": "1.0.0",
  "description": "runs ethereum-tests with VM and EVM",
  "keywords": [
    "ethereum",
    "VM",
    "EVM",
    "ethereum-tests"
  ],
  "homepage": "https://github.com/ethereumjs/ethereumjs-monorepo/tree/master/packages/evm-vm-tests#readme",
  "bugs": {
    "url": "https://github.com/ethereumjs/ethereumjs-monorepo/issues?q=is%3Aissue+label%3A%22package%3A+evm-vm-tests%22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ethereumjs/ethereumjs-monorepo.git"
  },
  "license": "MPL-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "../../config/cli/ts-build.sh",
    "clean": "../../config/cli/clean-package.sh",
    "docs:build": "typedoc --options typedoc.js",
    "lint": "../../config/cli/lint.sh",
    "lint:diff": "../../config/cli/lint-diff.sh",
    "lint:fix": "../../config/cli/lint-fix.sh",
    "prepublishOnly": "../../config/cli/prepublish.sh",
    "tape": "tape -r ts-node/register --stack-size=1500",
    "test": "npm run tape -- './tests/**/*.spec.ts'",
    "test:browser": "karma start karma.conf.js",
    "tsc": "../../config/cli/ts-compile.sh"
  },
  "dependencies": {
    "@ethereumjs/evm": "^1.0.0",
    "@ethereumjs/vm": "6.0.0",
    "@types/core-js": "^2.5.0",
    "@types/node-dir": "^0.0.34",
    "debug": "^4.3.3"
  },
  "devDependencies": {
    "tape": "^5.3.1",
    "ts-node": "^10.2.1",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": ">=14"
  }
}
